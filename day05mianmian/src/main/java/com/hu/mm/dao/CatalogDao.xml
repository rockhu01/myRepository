<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hu.mm.dao.CatalogDao">
    <!--查询目录列表 List<Catalog> selectByCourseId(int courseId);-->
    <select id="selectByCourseId" parameterType="int" resultType="Catalog">
        select  id,name from t_catalog where course_id = #{CourseId}
    </select>

</mapper>
